<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="action_vehicle_type" model="ir.actions.act_window">
            <field name="name">Vehicle Type</field>
            <field name="res_model">service.vehicle.type</field>
            <field name="view_mode">list,form</field>
            <field name="help" type="html">
                <p class="o_view_nocontent_smiling_face">
                    Create a New Vehicle Type
                </p>
            </field>
        </record>
        <record id="vehicle_type_view_form" model="ir.ui.view">
            <field name="name">service.vehicle.type.view.form</field>
            <field name="model">service.vehicle.type</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <field name="name"/>

                        </group>
                    </sheet>
                </form>
            </field>
        </record>
        <record id="vehicle_type_view_tree" model="ir.ui.view">
            <field name="name">service.vehicle.type.view.list</field>
            <field name="model">service.vehicle.type</field>
            <field name="arch" type="xml">
                <list>
                    <field name="name"/>
                </list>
            </field>
        </record>
    </data>
</odoo>